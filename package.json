{
  "name": "prehacks",
  "version": "1.0.0",
  "description": "Hackathon applications and projects repository",
  "private": true,
  "workspaces": [
    "apps/*/ui"
  ],
  "scripts": {
    "install:all": "npm install && npm run install:apps",
    "install:apps": "cd apps/travel/beacon/ui && npm install",
    "build:all": "npm run build:apps",
    "build:apps": "cd apps/travel/beacon/ui && npm run build",
    "test:all": "npm run test:apps",
    "test:apps": "cd apps/travel/beacon/ui && npm test",
    "lint:all": "npm run lint:apps",
    "lint:apps": "cd apps/travel/beacon/ui && npm run lint",
    "start:beacon": "cd apps/travel/beacon && ./start_all.sh",
    "dev:beacon": "cd apps/travel/beacon/ui && npm run dev"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-username/prehacks.git"
  },
  "keywords": [
    "hackathon",
    "applications",
    "travel",
    "ai",
    "microservices"
  ],
  "author": "Your Name",
  "license": "MIT"
}
