{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "app/api/analyze-food/route.ts": {
      "maxDuration": 30
    },
    "app/api/analyze-barcode/route.ts": {
      "maxDuration": 10
    }
  }
}
